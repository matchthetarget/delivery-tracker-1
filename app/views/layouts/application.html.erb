<!DOCTYPE html>
<html>
  <head>
    <title>Target: Delivery Tracker 1</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>

    <link rel="stylesheet" href="/css/my_styles.css">
  </head>

  <body>
    <% if notice.present? %>
      <p><%= notice %></p>
    <% end %>

    <% if alert.present? %>
      <p><%= alert %></p>
    <% end %>

    
    <%= yield %>
  </body>
</html>
